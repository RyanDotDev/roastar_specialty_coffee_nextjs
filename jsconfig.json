{
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        /* '/src/*'' paths */
        "./src/*",
        "./src/lib/*", 
        "./src/pages/*",
        "./src/store/*", 
        "./src/styles/*", 
        "./src/theme/*", 
        /* '/src/app/*' paths */
        "./src/app/*",
        "./src/lib/api/*",
        "./src/app/utils/*",
        "./src/app/pages/*",
      ]
    },
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "**/*.js",
    "**/*.jsx",
    "src/layout.jsx",
    "src/page.jsx",
    ".next/types/**/*.ts"
, "src/pages/api/shopify/route.js", "src/lib/api/shopify/products.js" , "next.config.mjs", "next.config.mjs" ],
  "exclude": []
}